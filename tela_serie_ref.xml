<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/anofundo">

    <Button
        android:id="@+id/btn5"
        android:layout_width="230dp"
        android:layout_height="230dp"
        android:background="@drawable/btn5clicar"
        android:layout_marginLeft="152dp"
        android:layout_marginStart="152dp"
        android:layout_marginBottom="40dp"
        android:layout_above="@+id/btn7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="230dp"
        android:layout_height="230dp"
        android:background="@drawable/btn6clicar"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:layout_toRightOf="@+id/btn5"
        android:layout_toEndOf="@+id/btn5"
        android:layout_alignBottom="@+id/btn5" />

    <Button
        android:id="@+id/btn7"
        android:layout_width="230dp"
        android:layout_height="230dp"
        android:background="@drawable/btn7clicar"
        android:layout_alignLeft="@+id/btn5"
        android:layout_alignStart="@+id/btn5"
        android:layout_alignBottom="@+id/btn8" />

    <Button
        android:id="@+id/btn8"
        android:layout_width="230dp"
        android:layout_height="230dp"
        android:background="@drawable/btn8clicar"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/btn6"
        android:layout_alignStart="@+id/btn6" />

    <Button
        android:id="@+id/btn9"
        android:layout_width="230dp"
        android:layout_height="230dp"
        android:background="@drawable/btn9clicar"
        android:layout_below="@+id/btn8"
        android:layout_alignLeft="@+id/btn7"
        android:layout_alignStart="@+id/btn7"
        android:layout_marginTop="28dp" />
    <Button
        android:id="@+id/voltar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="41dp"
        android:background="@drawable/voltar2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp" />

</RelativeLayout>
